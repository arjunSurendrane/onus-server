{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "standard-with-typescript",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "comma-dangle": "off",
    "semi": "off",
    "curly": "warn",
    "no-unused-vars": "warn",
    "no-console": "warn",
    "space-before-function-paren": "off",
    "eqeqeq": "off"
  }
}
